{
  "name": "language-xi",
  "displayName": "Xi markup language",
  "description": "Syntax highlighting and wikiword jumps supoort for Xi markup language",
  "repository": "https://github.com/grigoryvp/vscode-language-xi",
  "icon": "icon.png",
  "version": "1.0.11",
  "publisher": "grigoryvp",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:xi"
  ],
  "scripts": {
    "build": "vsce package",
    "publish": "chrome https://marketplace.visualstudio.com/manage"
  },
  "main": "./extension.js",
  "files": [
    "*.js",
    "xi.tmLanguage.json"
  ],
  "contributes": {
    "languages": [
      {
        "id": "xi",
        "extensions": [
          ".xi"
        ]
      }
    ],
    "grammars": [
      {
        "language": "xi",
        "scopeName": "text.xi",
        "path": "./xi.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "vsce": "^1.47.0"
  }
}
